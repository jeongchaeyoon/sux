<table name="board">
  <column name="id" type="int" size="12" notnull="notnull" auto_increment="auto_increment" primary_key="primary key"/>
  <column name="category" type="varchar" size="20" notnull="not null"/>
  <column name="is_notice" type="char" size="1" default="n" notnull="not null"/>
  <column name="user_id" type="varchar" size="75" notnull="not null"/>
  <column name="user_name" type="varchar" size="75" notnull="not null"/>
  <column name="nick_name" type="varchar" size="75" notnull="not null"/>
  <column name="password" type="varchar" size="75" notnull="not null"/>
  <column name="title" type="varchar" size="200" notnull="not null"/>
  <column name="contents" type="text" notnull="not null"/>
  <column name="email_address" type="varchar" size="50" notnull="not null"/>
  <column name="date" type="datetime"/>
  <column name="ip" type="varchar" size="30"/>
  <column name="readed_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="voted_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="blamed_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="progress_step" type="varchar" size="12" notnull="null"/>
  <column name="igroup_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="space_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="ssunseo_count" type="int" size="12" default="0" notnull="not null"/>
  <column name="wall" type="char" size="1"/>
  <column name="contents_type" type="char" size="4" default="text" notnull="not nul"/>
  <column name="filename" type="varchar" size="50"/>
  <column name="filesize" type="varchar" size="50"/>
  <column name="filetype" type="varchar" size="50"/>  
</table>